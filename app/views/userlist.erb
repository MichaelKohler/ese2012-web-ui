<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Users</title>
    <%= includeERB('head.erb') %>
  </head>
  <body>
    <%= includeERB('nav.erb') %>
    <div class="container">
      <section id="content">
        <div class="row">
          <%= includeERB('sidebar.erb') %>
          <div class="span10">
            <h1>Users</h1>
            <ul style="margin-top: 25px;">
              <% for user in @users %>
              <li><a href="/profile/<%= user.name %>"><%= user.name %></a></li>
              <% end %>
            </ul>
          </div>
        </div>
      </section>
    </div>

    <%= includeERB('libs.erb') %>
